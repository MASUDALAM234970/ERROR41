
#include<stdio.h>

int main()
{
    int array []={2,9,5,5,2,41};
    int i, j, s=6;
    int count=0;
    int temp =0;
    int flag =0;
    for(i=0; i<s-1; i++)
    {
        count++;
        for (j=0; j<s-1; j++)
        {   
            flag++;
        
            if(array[j]>array[j+1])
            {
           
                temp=array[i];
                array[i]=array[j+1];
                array [j+1]=temp ;
            }
    
        }
    
    }
    
    printf("Total loop = %d\n",count);

    for(i=0; i<s;i++)
    {
      printf("%d ", array[i]);
    }
    
 printf("\nTotal loop 2 =%d\n", flag);
}
//Bubble short
