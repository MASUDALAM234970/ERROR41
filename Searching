#include<stdio.h>
#include<stdlib.h>
int main ()
{
        
          int i, j,n,value;

          printf("Please ! Give me array size :\n");
          scanf("%d",&n);
           int array[n];
            printf("Please ! Enter array Digit :\n");
            for ( i = 0; i <n; i++)
            {
                scanf("%d",&array[i]);
            }
            printf("Enter Searching element ");
            scanf("%d",&value);


       // int array[]={10,15,20,25,30,40,50,60};
       // int value=400;
       // int count=8;
        for ( i = 0; i < n; i++)
        {
            if (array[i]==value)
            {
                 printf("%d is present at index %d.\n", value, i);

                  break;
            }
            
           
        }
    
        if (i == n) {
        printf("%d is not present in the array.\n", value);
    }
       
        return 0;





}
